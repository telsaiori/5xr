<%= simple_form_for(@user) do |f| %>


  <%= f.input :name,placeholder:'name',label:false %>
  <%= f.input :email,placeholder:'email',label:false %>
  <%= f.input :phone,placeholder:'email',label:false%>
  <%=f.input :photo%>
  <div class="actions">
    <%= f.submit class:'btn btn-success' %>
  </div>
<% end %>
